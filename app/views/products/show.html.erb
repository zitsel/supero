<div id="content">

<div id="product-images-container">


    <div id="product-images">


        <div id="large-image">

            <div id="image-zoom-container" style="overflow: hidden; display: none;">
                <%=image_tag @product.ordered_photos.first.uploaded_file(:original), :style=>"position: absolute; top: -527.1978021978023px; left: -219.42857142857144px;"%>
            </div>
            <div id="medium-image-container" title="foo" style="cursor: crosshair;">
                <div id="image-zoom-marker" style="position: absolute; opacity: 0.7; height: 225px; width: 215px; display: none;">
                    
                </div>
                <%=image_tag @product.ordered_photos.first.uploaded_file(:large), :id=>"medium-image"%>
