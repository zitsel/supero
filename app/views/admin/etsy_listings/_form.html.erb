<%= form_tag admin_product_etsy_listings_path method: :post do |f| %>

Title: <%= text_field_tag(:title, @product.etsy_title)%><br />
Description:  <%= text_area_tag(:description, (render :partial => 'description'))%><br />
Who Made: <%= select_tag(:who_made, options_for_select(["i_did","someone_else","collective"],"someone_else"))%><br />
Quantity: <%= text_field_tag(:quantity, 1)%><br />
Price: <%= text_field_tag(:price, @product.price)%><br />
Shipping ID: <%= text_field_tag(:shipping_template_id, 2175562941)%><br />
Category ID: <%= text_field_tag(:category_id, 69164513)%><br />
Is Supply: <%= text_field_tag(:is_supply, false)%><br />
When Made: <%= select_tag(:when_made, options_for_select(["before_1995","1998_1994","1980s","1970s","1960s","1950s","1940s","1930s","1920s","1910s","1900s","1800s","1700s","before_1700"],))%><br />

  <%= submit_tag("Create etsy Listing")%>
<%end%>