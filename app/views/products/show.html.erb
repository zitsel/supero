            </div>

            <div class="nav-carousel-nav-button nav-carousel-next-button" id="product-next">
                <span class="hidden"></span>
            </div>
        </div>
        <!-- End navigation carousel -->

       
        </div><div class="clickableImages"><h6 class="image-count">Image <span id="carousel-image-count-current-image">5</span>/9</h6> | 
           <%=link_to "Full size Image", image_path(@product.main_photo), :id=>"full-size-image-link", :data=>{ :colorbox=>true, :colorbox_max_height=> '90%', :colorbox_max_width=>"90%"}%>
                </div>
        </div>
    </div>

</div>

