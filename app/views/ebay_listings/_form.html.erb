<%= simple_form_for @ebay_listing, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :product_id %>
  <%= f.input :ebay_item_id %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :insertion_fees %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ebay_listings_path, :class => 'btn' %>
  </div>
<% end %>
